            <th class="span2">Denominazione</th>
            <th class="span2">Prezzo vendita</th>
            <th class="span2"> Gruppo attributi</th>
            <th class="span2"></th>
        </tr>
    </thead>
    <tbody>
        {%for item in articoli%}
        <tr class="first">
            <td> {{item.id}} </td>
            <td>{{item.denominazione}}</td>
